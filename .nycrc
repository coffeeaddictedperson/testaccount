{
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/js/**/*.ts",
      "src/js/**/*.tsx"
    ],
    "exclude": [
      "node_modules"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": false,
    "check-coverage": true,
    "statements": 20,
    "functions": 10,
    "branches": 20,
    "lines": 20
}
